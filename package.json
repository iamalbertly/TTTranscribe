    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.9",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
